<p id="notice"><%= notice %></p>

<h1>Listing Packages</h1>

<table>
  <thead>
    <tr>
      <th>Bike Id</th>
      <th>Message</th>
      <th>Created at</th>
    </tr>
  </thead>

  <tbody>
    <% @packages.each do |package| %>
      <tr>
        <td><%= package.bike_id %></td>
        <td><%= package.message %></td>
        <td><%= package.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
